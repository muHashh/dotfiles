  "workbench.startupEditor": "none",
  "editor.minimap.enabled": false,
  "workbench.colorCustomizations": {
    "editor.background": "#121212",
    "tab.activeBackground": "#121212",
    "tab.activeBorderTop": "#121212",
    // "tab.activeForeground": "#DDDDDD",
    // "tab.inactiveBackground": "#191723",
    //   "terminal.background": "#191723",
    //   "panel.background": "#191723",
  },
  "workbench.iconTheme": "vs-minimal",
  "[python]": {
    "editor.formatOnType": true
  },
  "go.toolsManagement.autoUpdate": true,
  "python.defaultInterpreterPath": "/usr/local/bin/python3",
  // "clangd.path": "/Users/hassan/Library/Application Support/Code/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/16.0.2/clangd_16.0.2/bin/clangd",
  "files.exclude": {},
  "files.watcherExclude": {},
  "editor.fontLigatures": true,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false
  },
  // "C_Cpp.intelliSenseEngine": "disabled",
  // "[cpp]": {
  //   "editor.wordBasedSuggestions": false,
  //   "editor.suggest.insertMode": "replace",
  //   "editor.semanticHighlighting.enabled": true
  // },
  // "C_Cpp.intelliSenseEngineFallback": "enabled",
  // "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.fontSize": 14,
  "workbench.layoutControl.enabled": false,
  "workbench.activityBar.location": "top",
  "editor.fontSize": 14,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "telemetry.telemetryLevel": "off",
  "gitlens.telemetry.enabled": false,
  "workbench.colorTheme": "Modern Dark Monokai",
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "vim.smartRelativeLine": true,
  "window.commandCenter": false,
  "terminal.integrated.defaultProfile.osx": "zsh",
}